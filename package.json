{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "npm run build:local && npm run serve",
    "test": "jest",
    "build": "npm run build:local && npm run build:rollup",
    "build:local": "npm run build:typescript && npm run generate && npm run build:copy",
    "build:typescript": "tsc",
    "build:rollup": "rollup -c",
    "build:copy": "node ./build/tools/copy-assets.js",
    "serve": "npm run serve:typescript & npm run serve:app",
    "serve:typescript": "tsc --watch --preserveWatchOutput",
    "serve:app": "wds --node-resolve --port=8081",
    "deploy": "npm run build && firebase deploy",
    "generate": "node screenshot.js",
    "generate:screenshot": "node screenshot.js screenshot",
    "generate:gif": "node screenshot.js gif"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "esm-seedrandom": "^3.0.5",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@web/dev-server": "^0.4.1",
    "@web/dev-server-rollup": "^0.6.1",
    "del": "^3.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^27.6.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "image-size": "^0.9.3",
    "jest": "^29.7.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "png-file-stream": "^1.2.1",
    "puppeteer": "^1.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5",
    "wct-mocha": "^1.0.0"
  }
}
